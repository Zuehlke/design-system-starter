{
  "name": "@zuhlke/design-system-components",
  "version": "0.65.0",
  "type": "module",
  "main": "dist/index.js",
  "web-types": "dist/web-types.json",
  "customElements": "dist/custom-elements.json",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "precheck": "npm run prebuild",
    "check": "tsc --noEmit --emitDeclarationOnly false",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "analyze": "lit-analyzer",
    "prebuild": "npm run design-tokens:generate && npm run icons:generate",
    "build": "vite build",
    "storybook": "storybook dev -p 6006 --no-open",
    "prebuild-storybook": "npm run build",
    "build-storybook": "storybook build",
    "test": "vitest run --reporter=verbose --reporter=junit --coverage",
    "test:watch": "vitest",
    "design-tokens:generate": "node ./tokens/style-dictionary.cjs",
    "icons:generate": "node src/components/icon/generate-icon-types.js"
  },
  "dependencies": {
    "@floating-ui/dom": "1.5.3",
    "@neodrag/vanilla": "2.0.3",
    "@tanstack/match-sorter-utils": "8.8.4",
    "@tanstack/table-core": "8.10.3",
    "csv": "6.3.3",
    "highcharts": "11.1.0",
    "lit": "2.8.0",
    "lit-html": "2.8.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.0",
    "@ctrl/tinycolor": "3.6.0",
    "@easepick/core": "1.2.1",
    "@easepick/kbd-plugin": "1.2.1",
    "@easepick/range-plugin": "1.2.1",
    "@lit-labs/virtualizer": "2.0.7",
    "@open-wc/testing-helpers": "2.3.0",
    "@storybook/addon-actions": "7.6.17",
    "@storybook/addon-essentials": "7.6.17",
    "@storybook/addon-links": "7.6.17",
    "@storybook/addons": "7.6.17",
    "@storybook/blocks": "7.6.17",
    "@storybook/theming": "7.6.17",
    "@storybook/web-components": "7.6.17",
    "@storybook/web-components-vite": "7.6.17",
    "@testing-library/dom": "9.3.3",
    "@testing-library/jest-dom": "6.1.3",
    "@testing-library/user-event": "14.5.1",
    "@typescript-eslint/eslint-plugin": "6.7.3",
    "@vitest/coverage-v8": "1.3.1",
    "@web-types/lit": "2.0.0-3",
    "babel-loader": "9.1.3",
    "cem-plugin-expanded-types": "1.2.0",
    "custom-element-jet-brains-integration": "1.4.4",
    "date-fns": "2.30.0",
    "element-internals-polyfill": "1.3.8",
    "eslint": "8.50.0",
    "eslint-config-standard-with-typescript": "39.1.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-n": "16.1.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-storybook": "0.6.14",
    "globby": "13.2.2",
    "inter-ui": "3.19.3",
    "jsdom": "22.1.0",
    "pascal-case": "3.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "shadow-dom-testing-library": "1.11.0",
    "storybook": "7.6.17",
    "style-dictionary": "3.8.0",
    "ts-lit-plugin": "2.0.2",
    "typescript": "5.3.3",
    "vite": "5.1.5",
    "vite-plugin-cem": "0.7.0",
    "vite-plugin-dts": "3.7.3",
